# EmailJS Configuration for Rifah Laboratories LIMS
# Copy this file to .env and fill in your actual EmailJS credentials

# EmailJS Service ID (from EmailJS dashboard)
VITE_EMAILJS_SERVICE_ID=your_service_id_here

# EmailJS Template ID for Welcome Emails
VITE_EMAILJS_TEMPLATE_ID=your_welcome_template_id_here

# EmailJS Public Key
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here

# EmailJS Template ID for Security Alerts
VITE_EMAILJS_SECURITY_TEMPLATE_ID=your_security_template_id_here

# Laboratory Information (optional - can be configured in settings)
VITE_LAB_NAME=Rifah Laboratories
VITE_LAB_ADDRESS=170- Hali Road Tehseel Chowk Sahiwal
VITE_LAB_PHONE=0404-220285
VITE_LAB_EMAIL=info@rifahlabs.com
VITE_LAB_WEBSITE=www.rifahlabs.com 